{
  "name": "@kairos/backend",
  "version": "1.0.0",
  "private": true,
  "description": "Backend do projeto Kairos usando Fastify",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "update": "pnpm up --latest",
    "format": "biome format --write src/",
    "lint": "biome check --apply src/",
    "check": "biome check src/",
    "clean": "rimraf dist node_modules",
    "seed": "tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@fastify/auth": "^5.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "date-fns": "^4.1.0",
    "fastify": "^5.1.0",
    "fastify-type-provider-zod": "^4.0.2",
    "openai": "^4.73.1",
    "pino-pretty": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "axios": "^1.7.8",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
